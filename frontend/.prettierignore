# Ignore editor directories
.vscode/
.idea/

# Ignore node_modules directory
node_modules/

# Ignore Angular build and temporary directories
.angular/
dist/
build/

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment variable files
.env
.env.local
.env.*.local

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore Rust target and build directories
target/
debug/
release/

# Ignore Rust dependency lock file
Cargo.lock

# Ignore npm dependency lock file
package-lock.json

# Ignore yarn lock file (if you use yarn)
yarn.lock
